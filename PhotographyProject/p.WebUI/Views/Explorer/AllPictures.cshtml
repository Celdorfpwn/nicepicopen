@model IEnumerable<p.Database.Concrete.Entities.Picture>

@foreach (var picture in Model)
    {
        <div class="albumPictureDiv">
            <div class="albumPictureContainer">
            <a href='@Url.Action("Album", "Explorer", new { id = picture.AlbumId,picture = picture.Id })'><img class="albumPicture" src="@Url.Action("AlbumPictureCarusel", "Images", new { id = picture.Id })" /></a>
               </div>
        </div>
}