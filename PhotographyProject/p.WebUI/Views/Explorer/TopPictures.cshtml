@model IEnumerable<p.Database.Concrete.Entities.Picture>

@foreach (var picture in Model)
{
    <a href="@Url.Action("Album", new { id = picture.AlbumId, picture = picture.Id })">
    <img class="albumPicture" src="@Url.Action("AlbumPictureCarusel", "Images", new { id = picture.Id})" />
        </a>
}