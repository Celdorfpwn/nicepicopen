@model IEnumerable<p.Database.Concrete.Entities.Picture>


    @foreach (var picture in Model)
    {
            <div class="albumDetailsPictureContainer">
            <a href='@Url.Action("PictureProfile", "WorkbenchPictures", new { id = picture.Id })'><img class="albumDetailsPicture" src="@Url.Action("AlbumPictureCarusel", "Images", new { id = picture.Id })" /></a>
               </div>
    }